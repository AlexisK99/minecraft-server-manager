{
  "name": "minecraft-server-manager",
  "version": "1.12.0",
  "description": "Minecraft Server Manager",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start-dev": "pushd minecraftservermanager && ./node_modules/.bin/react-scripts start && popd",
    "build-web": "pushd minecraftservermanager && ./node_modules/.bin/react-scripts build && popd",
    "test-backend": "./node_modules/mocha/bin/_mocha -u bdd --colors --reporter mochawesome --reporter-options reportDir=./src/tests/results src/test",
    "test-web": "pushd minecraftservermanager && ./node_modules/.bin/react-scripts test --env=jsdom && popd",
    "eject": "pushd minecraftservermanager && ./node_modules/.bin/react-scripts eject && popd"
  },
  "proxy": "http://localhost:3001",
  "repository": {
    "type": "git",
    "url": "git@github.com:nickrnet/minecraft-server-manager.git"
  },
  "author": {
    "name": "nickrnet",
    "url": "https://github.com/nickrnet/"
  },
  "dependencies": {
    "acorn": "^6.2.0",
    "ajv": "^6.10.2",
    "ajv-keywords": "^3.4.1",
    "archiver": "^3.0.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "fs-extra": "^8.1.0",
    "passport": "^0.4.0",
    "permission": "^1.1.0",
    "prop-types": "^15.7.2",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "expect": "^24.8.0",
    "jsdoc": "^3.6.3",
    "mocha": "^6.2.0",
    "mochawesome": "^4.0.1",
    "mock-fs": "^4.10.1",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": ">=10.16.0"
  }
}
